        <addForeignKeyConstraint baseColumnNames="assessor_id"
                                 baseTableName="agency_notice"
                                 constraintName="fk_agency_notice__assessor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dealer"/>
