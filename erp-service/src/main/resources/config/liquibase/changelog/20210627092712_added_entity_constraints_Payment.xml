<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Payment.
    -->
    <changeSet id="20210627092712-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="payment_requisition_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_payment_requisition_id"
                                 referencedColumnNames="id"
                                 referencedTableName="payment_requisition"/>

        <addForeignKeyConstraint baseColumnNames="tax_rule_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_tax_rule_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tax_rule"/>

        <addForeignKeyConstraint baseColumnNames="payment_category_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment_payment_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="payment_category"/>

    </changeSet>
</databaseChangeLog>
